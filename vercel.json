{
  "version": 2,
  "rewrites": [
    {
      "source": "/proxy/(.*)",
      "destination": "https://downloads.immortalwrt.org/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/proxy"
    }
  ],
  "redirects": [
    {
      "source": "/blocked",
      "destination": "/blocked.html",
      "statusCode": 200
    }
  ],
  "headers": [
    {
      "source": "/blocked.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, max-age=0" },
        { "key": "Content-Type", "value": "text/html; charset=utf-8" }
      ]
    },
    {
      "source": "/api/proxy",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=utf-8" }
      ]
    }
  ],
  "regions": ["hkg1"]
}